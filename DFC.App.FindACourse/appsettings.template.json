{
  "Logging": {
    "LogLevel": {
      "Default": "Error"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "Configuration": {
    "ApplicationName": "DFC-App-Find-a-course",
    "CosmosDbConnections": {
      "ContentPages": {
        "AccessKey": "[KeyHere]",
        "EndpointUrl": "https://localhost:8081",
        "DatabaseId": "dfc-app-conpage",
        "CollectionId": "pages",
        "PartitionKey": "/PartitionKey"
      }
    },
    "CourseSearchClient": {
      "CosmosAuditConnection": {
        "AccessKey": "[KeyHere]",
        "EndpointUrl": "https://localhost:8081",
        "DatabaseId": "dfc-digital-audit",
        "CollectionId": "CourseSearchAuditRecords",
        "PartitionKey": "/PartitionKey"
      },
      "CourseSearchSvc": {
        "ServiceEndpoint": "https://dev.api.nationalcareersservice.org.uk/coursedirectory/findacourse/",
        "APIKey": "[KeyHere]",
        "SearchPageSize": 20,
        "RequestTimeOutSeconds": 10,
        "TransientErrorsNumberOfRetries": 3
      },
      "Policies": {
        "HttpCircuitBreaker": {
          "DurationOfBreak": "00:00:10",
          "ExceptionsAllowedBeforeBreaking": 3
        },
        "HttpRetry": {
          "BackoffPower": 2,
          "Count": 3
        }
      }
    }
  },
  "ApplicationInsights": {
    "InstrumentationKey": "[instrumentation Key]"
  },
  "ServiceBusOptions": {
    "ServiceBusConnectionString": "[service bus connection string]",
    "topicName": "job-profile-refresh"
  }
}