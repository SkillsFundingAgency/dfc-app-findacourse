@model IndexViewModel

<html>
    <body>
        @if (Model.Documents?.Count() > 0)
        {
            <ul>
                @foreach (var document in Model.Documents)
                {
                    <li>
                        <a href="/find-a-course/@document.CanonicalName">@document.CanonicalName</a>
                    </li>
                }
            </ul>
        }
    </body>
</html>